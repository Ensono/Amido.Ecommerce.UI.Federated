{
  "name": "@batman/cache-manager",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "serve": "ts-node -T ./src/server.ts",
    "azurite:install": "ts-node -T ./src/scripts/install-azurite.ts",
    "azurite:start": "azurite --location ./azurite --debug ./azurite/debug.log",
    "test": "node ../../scripts/test.js --coverage --watchAll=false",
    "test:watch": "node ../../scripts/test.js --watch"
  },
  "dependencies": {
    "@azure/data-tables": "^13.2.0",
    "@babel/core": "^7.16.0",
    "@batman/core-logger": "*",
    "@testing-library/jest-dom": "^5.16.1",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.13",
    "axios": "^0.26.0",
    "babel-jest": "^27.4.2",
    "chalk": "^4.1.2",
    "dotenv": "^16.0.3",
    "express": "^4.17.2",
    "jest": "^27.4.3",
    "react-dom": "^18.0.0",
    "supertest": "^6.3.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@types/jest": "^29.1.1",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.0",
    "@types/supertest": "^2.0.12",
    "babel-plugin-styled-components": "^2.0.2",
    "eslint-webpack-plugin": "^3.1.1",
    "jest-coverage-ratchet": "^0.2.3",
    "nodemon": "^2.0.15",
    "react-ssr-dev-utils": "^2.2.1",
    "ts-node": "^10.4.0",
    "url-loader": "^4.1.1"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "@babel/plugin-transform-react-jsx",
        {
          "runtime": "automatic"
        }
      ]
    ]
  }
}
